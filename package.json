  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsimons/Game-Watch/issues"
  },
  "homepage": "https://github.com/tsimons/Game-Watch",
  "dependencies": {
    "bootstrap": "^3.3.2",
    "express": "^4.10.7",
    "flux": "^2.0.1",
    "lodash": "^2.4.1",
    "react": "^0.12.2",
    "react-bootstrap": "^0.16.1",
    "react-router": "~0.11.6",
    "react-tools": "^0.12.2",
    "socket.io": "^1.2.1"
  },
  "devDependencies": {
    "babel-jest": "^4.0.0",
    "debug": "^2.1.1",
    "gulp": "^3.8.10",
    "gulp-babel": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-less": "^3.0.0",
    "gulp-livereload": "^3.2.0",
    "gulp-notify": "^2.1.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.0.2",
    "react-tools": "^0.13.1",
    "should": "^4.4.4",
    "sinon": "^1.14.1",
    "socket.io-client": "^1.2.1",
    "supertest": "^0.15.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
